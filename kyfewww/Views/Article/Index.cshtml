@model IEnumerable<kyfelib.Article>
@{
	ViewBag.Title = "YKFE Articles";
}

@section scripts {
	<script type="text/javascript">
		$(document).ready(function() {

		});
	</script>
}
<div id="body">
	<section class="featured">
		<div
			class="content-wrapper">
			<h2>Articles </h2>
			<button class="btn" data-toggle="modal" data-target="#editForm">Add New</button>
			<table id="articles" class="table table-striped table-hover">
				<thead>
					<tr>
						<th>Date </th>
						<th>Title </th>
						<th>Author </th>
					</tr>
				</thead>
				<tbody>
					@foreach (kyfelib.Article article in Model)
					{
						<tr>
							<td>@Html.ActionLink(article.RowKey, "Index", new {id=article.RowKey})</td>
							<td>@article.Name</td>
							<td>@article.Author</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</section>
</div>
